<div class="page-width">
  <header class="section-header text-center">
    <h1 class="section-header__title h2">{{ page.title }}</h1>
    <style>
    .feature-row {
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    width: 100%;
    -ms-flex-align: stretch;
    -webkit-align-items: stretch;
    -moz-align-items: stretch;
    -ms-align-items: stretch;
    -o-align-items: stretch;
    align-items: stretch;
}
      .email-title-box {
      	margin: 15px 0;
      }
      .email-title-box h2 {
        color: #ffffff;
        margin-top: 25px;
      }
      .form-builder-box {
        min-height: auto; 
      }
      .bottom-banner-box {
      	margin-top: 100px;
      }
      .product-side-box1 {
        margin: 0 20px 0 auto;
        box-shadow: 0px 0px 5px 5px rgba(0,0,0,0.1);
        padding-top: 25px;
      }
        .product-side-box1-child {	
          padding: 15px;
        }
      .product-side-box2 {
        margin: 0 auto;
        box-shadow: 0px 0px 5px 5px rgba(0,0,0,0.1);
        padding-top: 25px;
      }
      	.product-side-box2-child {	
          padding: 15px;
        }
      @media only screen and (max-width: 767.98px) {
        .feature-row {flex-wrap:wrap;    margin-top: 0px !important;}
        .form-builder-box {min-height: auto;}
        .page-width {padding: 30px 0px 0px;}
        .email-title-box  {margin: 0px;padding-top: 20px;}
        .product-side-box1-child {margin-top: 0px !important;}
        .bottom-banner-box {margin-top: 60px;}
        .product-side-box { margin: 0;}
        .product-side-box1 {margin: 0 auto 20px;margin-top: 30px;padding-bottom: 25px; }
        .product-side-box2 {display: table;padding-bottom:25px;}
        .feature-row__image {margin: 0;}
        .email-code-part {margin-top:10px !important;}
      }
            
      @media only screen and (min-width: 768px) and (max-width:1023.98px) {
         .product-side-box1.product-side-box2, h2 {
            font-size: 2.9rem;
          }
       }
      
    </style>
  </header>
  
  <script src="{{ 'recommendation-v1.js' | asset_url }}" defer="defer"></script>
  
